{
  "name": "Dark Mode Toggle",
  "description": "Adds a dark mode toggle to websites with customizable themes",
  "version": "1.0.0",
  "category": "productivity",
  "matches": ["<all_urls>"],
  "contentScript": "content.js",
  "cssFile": "content.css",
  "settings": {
    "defaultTheme": "dark",
    "autoDetect": true,
    "excludeSites": ["youtube.com", "twitter.com"]
  }
}
